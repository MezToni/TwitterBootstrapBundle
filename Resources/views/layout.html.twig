{% extends '::base.html.twig' %}

{% block body %}
    <div class="container">
        {% block container %}{% endblock %}
        <footer>{% block footer %}<p>&copy; Company 2012</p>{% endblock %}</footer>
    </div>
{% endblock %}

{% block stylesheets %}
    {{ include('TwitterBootstrapBundle::html5shiv.html.twig') }}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% block stylesheet_twitter_bootstrap %}
        <link href="{{ asset('bundles/twitterbootstrap/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}
    {% block stylesheets_twitter_bootstrap_responsive %}
        <link href="{{ asset('bundles/twitterbootstrap/css/bootstrap-responsive.min.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}
    {{ parent() }}
{% endblock %}


{% block javascripts %}
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js" type="text/javascript"></script>
  <script src="{{ asset('bundles/twitterbootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
  {{ parent() }}
{% endblock %}